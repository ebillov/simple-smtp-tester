# SMTP Configuration
SMTP_HOST=smtp.office365.com
SMTP_PORT=587
SMTP_USERNAME=
SMTP_PASSWORD=
# SMTP_SECURITY: starttls (port 587) or ssl (port 465) or none (no encryption)
SMTP_SECURITY=starttls
# SMTP_REQUIRE_AUTH: Set to false for relay servers that don't require authentication
SMTP_REQUIRE_AUTH=true
